<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="customize">
        <attr name="android:clickable"/>
        <attr name="android:hint"/>
        <attr name="android:radius"/>
        <attr name="isClear" format="boolean" />
    </declare-styleable>


    <declare-styleable name="CodeEditText">
        <attr name="strokeLength" format="integer" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="strokeHeight" format="dimension" />
        <attr name="strokePadding" format="dimension" />
        <attr name="strokeBackground" format="reference" />
    </declare-styleable>


    <declare-styleable name="SlideButton">
        <attr name="sBackgroundColorOff" format="color|reference" />
        <attr name="sBackgroundColorOn" format="color|reference" />
        <attr name="sButtonColor" format="color|reference" />
        <attr name="sMargin" format="dimension|reference" />
    </declare-styleable>
</resources>
